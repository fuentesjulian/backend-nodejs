<h1>Profile</h1>
<div class="container">
  <div class="row">
    <div class="col-md-6">
      <img src="{{user.img}}" />
    </div>
    <div class="col-md-6">
      <div class="row">
        <div class="col-md-3">Nombre:</div>
        <div class="col-md-9">{{user.first_name}}</div>
      </div>
      <div class="row">
        <div class="col-md-3">Apellido:</div>
        <div class="col-md-9">{{user.last_name}}</div>
      </div>
      <div class="row">
        <div class="col-md-3">Edad:</div>
        <div class="col-md-9">{{user.age}}</div>
      </div>
      <div class="row">
        <div class="col-md-3">email:</div>
        <div class="col-md-9">{{user.email}}</div>
      </div>
      <div class="row">
        <div class="col-md-3">rol:</div>
        <div class="col-md-9">{{user.role}}</div>
      </div>
    </div>
  </div>
</div>